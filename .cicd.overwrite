---

# Generic
generic:
  role: ansible-role-cron
  role_short: cron
  role_description: |
    Installs & manages cron
  collections: []

# Dependencies
dependencies:
  roles: []
  collections: []

# Molecule
molecule:
  converge:
    vars: {}
  host_vars_static:
    cron-rockylinux8-node1:
      cron_custom_config:
        - { key: TZ, value: 'Europe/Helsinki' }
   
